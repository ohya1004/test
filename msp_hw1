;=======================================
 label13: 
        ;紅亮 綠暗 2 L
        mov.b  #00000001b , P1OUT
        mov.b  #00000000b , P4OUT
                                                   
        MOV #1000 , R7    
;==============Delay14迴圈===============
Delay14_2:
