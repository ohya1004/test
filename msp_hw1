        MOV #525 , R8 
        cmp #0 , R7
        jnz Delay16
        jz label16
Delay16:
        sub #1  , R8
        cmp #0  , R8
        jnz Delay16
        jz Delay16_2   
;=======================================        
label16:
        ;紅暗 綠暗 0.6 O
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
        
        MOV #1000 , R5    
;==============Delay17迴圈===============
Delay17_2:
        sub #1  , R5
        MOV #159 , R6 
        cmp #0 , R5
        jnz Delay17
        jz label17
Delay17:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay17
        jz Delay17_2   
;=======================================               
 label17: 
        ;紅亮 綠暗 2 P
        mov.b  #00000001b , P1OUT
        mov.b  #00000000b , P4OUT
                                                   
        MOV #1000 , R7    
;==============Delay18迴圈===============
Delay18_2:
        sub #1  , R7
        MOV #525 , R8 
        cmp #0 , R7
        jnz Delay18
        jz label18
Delay18:
        sub #1  , R8
        cmp #0  , R8
        jnz Delay18
        jz Delay18_2   
;=======================================        
label18:
        ;紅暗 綠暗 0.6 Q
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
        
        MOV #1000 , R5    
;==============Delay19迴圈===============
Delay19_2:
        sub #1  , R5
        MOV #159 , R6 
        cmp #0 , R5
