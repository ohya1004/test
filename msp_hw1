        MOV #159 , R6 
        cmp #0 , R5
        jnz Delay9
        jz label9
Delay9:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay9
        jz Delay9_2   
;=======================================
label9:
        ;紅暗 綠亮 1 I
        mov.b  #00000000b , P1OUT
        mov.b  #10000000b , P4OUT   
       
        MOV #1000 , R5    
;==============Delay10迴圈===============
Delay10_2:
        sub #1  , R5
        MOV #264 , R6 
        cmp #0 , R5
        jnz Delay10
        jz label10
Delay10:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay10
        jz Delay10_2   
;=======================================
label10:
        ;紅暗 綠暗 5 
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
       
        MOV #1000 , R5    
;==============Delay11迴圈===============
Delay11_2:
        sub #1  , R5
        MOV #1312 , R6 
        cmp #0 , R5
        jnz Delay11
        jz label11
Delay11:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay11
        jz Delay11_2   
;=======================================
label11:
        ;紅暗 綠亮 1 J
        mov.b  #00000000b , P1OUT
        mov.b  #10000000b , P4OUT   
       
        MOV #1000 , R5    
;==============Delay12迴圈===============
Delay12_2:
        sub #1  , R5
