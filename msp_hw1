;=======================================    
label19: 
        ;紅亮 綠暗 2 R
        mov.b  #00000001b , P1OUT
        mov.b  #00000000b , P4OUT
                                                   
        MOV #1000 , R7    
;==============Delay20迴圈===============
Delay20_2:
        sub #1  , R7
        MOV #525 , R8 
        cmp #0 , R7
        jnz Delay20
        jz labelend
Delay20:
