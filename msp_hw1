        cmp #0  , R6
        jnz Delay15
        jz Delay15_2   
;=======================================  
 label15: 
        ;紅亮 綠暗 2 N
        mov.b  #00000001b , P1OUT
        mov.b  #00000000b , P4OUT
                                                   
        MOV #1000 , R7    
;==============Delay16迴圈===============
Delay16_2:
