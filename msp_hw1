        cmp #0  , R8
        jnz Delay20
        jz Delay20_2   
;=======================================        
        
labelend:       
       mov.b  #00000000b , P1OUT
       mov.b  #00000000b , P4OUT  
       JMP $
               
        
;=====================================================================        
        END
