#include "msp430.h"                     ; #define controlled include file
        NAME    main                    ; module name
        PUBLIC  main                    ; make the main label vissible
                                        ; outside this module
        ORG     0FFFEh
        DC16    init                    ; set reset vector to 'init' label
        RSEG    CSTACK                  ; pre-declaration of segment
        RSEG    CODE                    ; place program in 'CODE' segment
init:   MOV     #SFE(CSTACK), SP        ; set up stack
;===============主程式=======================================
main:   NOP                             ; main program
        MOV.W   #WDTPW+WDTHOLD,&WDTCTL  ; Stop watchdog timer
        
        ;LED1:red   LED2:green 
        ;紅亮 綠亮 5
        mov.b  #00000001b , P1DIR        
        mov.b  #00000001b , P1OUT
        mov.b  #10000000b , P4DIR
        mov.b  #10000000b , P4OUT
        MOV #1000 , R6    
        JMP Delay1_2
        
;===============Delay1迴圈==============       
Delay1_2:
        sub #1  , R6    ;1cycle
        MOV #1312 , R5   ;2cycle
        cmp #0 , R6     ;1cycle
        jnz Delay1      ;2cycle
        jz label1       ;2cycle  
Delay1:
        sub #1  , R5    ;1cycle
        cmp #0  , R5    ;1cycle
        jnz Delay1      ;2cycle
        jz Delay1_2     ;2cycle 
        
        
;=======================================         
 label1: 
        ;紅亮 綠暗 2 A
        mov.b  #00000001b , P1OUT
        mov.b  #00000000b , P4OUT
                                                   
        MOV #1000 , R7    
;==============Delay2迴圈===============
Delay2_2:
        sub #1  , R7
        MOV #525 , R8 
        cmp #0 , R7
        jnz Delay2
        jz label2
Delay2:
        sub #1  , R8
        cmp #0  , R8
        jnz Delay2
        jz Delay2_2   
;=======================================        
label2:
        ;紅暗 綠暗 0.6 B
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
        
        MOV #1000 , R5    
;==============Delay3迴圈===============
Delay3_2:
        sub #1  , R5
        MOV #159 , R6 
        cmp #0 , R5
        jnz Delay3
        jz label3
Delay3:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay3
        jz Delay3_2   
;=======================================                
label3:
        ;紅暗 綠亮 1 C
        mov.b  #00000000b , P1OUT
        mov.b  #10000000b , P4OUT   
        MOV #1000 , R5    
;==============Delay4迴圈===============
Delay4_2:
        sub #1  , R5
        MOV #264 , R6 
        cmp #0 , R5
        jnz Delay4
        jz label4
Delay4:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay4
        jz Delay4_2   
;=======================================      
label4:
        ;紅暗 綠暗 0.6 D
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
        MOV #1000 , R5    
;==============Delay5迴圈===============
Delay5_2:
        sub #1  , R5
        MOV #159 , R6 
        cmp #0 , R5
        jnz Delay5
        jz label5
Delay5:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay5
        jz Delay5_2   
;=======================================
label5:
        ;紅暗 綠亮 1 E
        mov.b  #00000000b , P1OUT
        mov.b  #10000000b , P4OUT   
        MOV #1000 , R5    
;==============Delay6迴圈===============
Delay6_2:
        sub #1  , R5
        MOV #264 , R6 
        cmp #0 , R5
        jnz Delay6
        jz label6
Delay6:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay6
        jz Delay6_2   
;=======================================
label6:
        ;紅暗 綠暗 0.6 F
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
       
        MOV #1000 , R5    
;==============Delay7迴圈===============
Delay7_2:
        sub #1  , R5
        MOV #159 , R6 
        cmp #0 , R5
        jnz Delay7
        jz label7
Delay7:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay7
        jz Delay7_2   
;=======================================
label7:
        ;紅暗 綠亮 1 G
        mov.b  #00000000b , P1OUT
        mov.b  #10000000b , P4OUT   
       
        MOV #1000 , R5    
;==============Delay8迴圈===============
Delay8_2:
        sub #1  , R5
        MOV #264 , R6 
        cmp #0 , R5
        jnz Delay8
        jz label8
Delay8:
        sub #1  , R6
        cmp #0  , R6
        jnz Delay8
        jz Delay8_2   
;=======================================
label8:
        ;紅暗 綠暗 0.6 H
        mov.b  #00000000b , P1OUT
        mov.b  #00000000b , P4OUT   
       
        MOV #1000 , R5    
;==============Delay9迴圈===============
Delay9_2:
